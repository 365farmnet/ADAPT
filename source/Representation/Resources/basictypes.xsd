<?xml version="1.0" encoding="utf-8"?><xs:schema xmlns:bt="urn:schemas-adapt-aggateway-org:BasicTypes" xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="urn:schemas-adapt-aggateway-org:BasicTypes" elementFormDefault="qualified" attributeFormDefault="unqualified" version="1.0.0.1">	<xs:simpleType name="uuid">		<xs:annotation>			<xs:documentation>Abstraction from format</xs:documentation>		</xs:annotation>		<xs:restriction base="bt:uuidFormal"/>	</xs:simpleType>	<xs:simpleType name="uuidFormal">		<xs:annotation>			<xs:documentation>Base 16 UUID.  '{' ,(1-8) time low, '-', (10-13)  time mid, '-', (15-18) time high and ver, '-', (20-23) clock_seq, '-', (25-36) node,'}'</xs:documentation>		</xs:annotation>		<xs:restriction base="xs:string">			<xs:pattern value="[{]?[0-9,a-f,A-F]{8}-[0-9,a-f,A-F]{4}-[0-9,a-f,A-F]{4}-[0-9,a-f,A-F]{4}-[0-9,a-f,A-F]{12}[}]?"/>		</xs:restriction>	</xs:simpleType>	<xs:simpleType name="uuidBase16">		<xs:annotation>			<xs:documentation>Base 16 UUID.  (0-7) time low, (8-11)  time mid, (12-15) time high and ver, (16-19) clock_seq, (20-31) node</xs:documentation>		</xs:annotation>		<xs:restriction base="xs:string">			<xs:pattern value="[0-9.a-f]{32}"/>		</xs:restriction>	</xs:simpleType>	<xs:simpleType name="uuidBase64">		<xs:annotation>			<xs:documentation>Base 64 binary encoded UUID</xs:documentation>		</xs:annotation>		<xs:restriction base="xs:base64Binary">			<xs:minLength value="24"/>			<xs:maxLength value="24"/>		</xs:restriction>	</xs:simpleType>	<xs:simpleType name="eridFormal">		<xs:restriction base="bt:uuidFormal"/>	</xs:simpleType>	<xs:simpleType name="eridBase16">		<xs:restriction base="bt:uuidBase16"/>	</xs:simpleType>	<xs:simpleType name="eridBase64">		<xs:restriction base="bt:uuidBase64"/>	</xs:simpleType>	<xs:simpleType name="erid">		<xs:restriction base="bt:eridFormal"/>	</xs:simpleType>	<xs:simpleType name="id">		<xs:restriction base="xs:int"/>	</xs:simpleType>	<xs:simpleType name="entityDefinedID">		<xs:restriction base="xs:int"/>	</xs:simpleType>	<xs:simpleType name="idRef">		<xs:restriction base="xs:int"/>	</xs:simpleType>	<xs:simpleType name="productionCycle">		<xs:restriction base="xs:string">			<xs:pattern value="[0-9]{4}"/>		</xs:restriction>	</xs:simpleType>	<xs:simpleType name="operationType">		<xs:restriction base="xs:NMTOKENS">			<xs:enumeration value="TillageOperation"/>			<xs:enumeration value="HarvestOperation"/>			<xs:enumeration value="SeedingOperation"/>			<xs:enumeration value="ApplicationOperation"/>			<xs:enumeration value="OtherOperation"/>		</xs:restriction>	</xs:simpleType>	<xs:simpleType name="nameType">		<xs:restriction base="xs:string">			<xs:whiteSpace value="preserve"/>			<xs:minLength value="1"/>			<xs:maxLength value="20"/>			<xs:pattern value="(\p{Lu}|\p{Ll}|\p{N}|\p{S}| |\{|\}|&amp;|&apos;|=|;|@|\[|~|!|[#-\)]|[+-\\]|\[|[\]-`]|[¡-©]|[«-±]|´|·|»|¿|×|÷| )*"/>		</xs:restriction>	</xs:simpleType>	<xs:simpleType name="commentType">		<xs:restriction base="xs:string">			<xs:whiteSpace value="preserve"/>			<xs:minLength value="0"/>			<xs:maxLength value="254"/>		</xs:restriction>	</xs:simpleType>	<xs:complexType name="BasicVersionInfoType">		<xs:attribute name="major" type="xs:int" use="required"/>		<xs:attribute name="minor" type="xs:int" use="required"/>	</xs:complexType>	<xs:complexType name="VersionInfoType">		<xs:complexContent>			<xs:extension base="bt:BasicVersionInfoType">				<xs:attribute name="build" type="xs:int" use="required"/>				<xs:attribute name="revision" type="xs:int" use="required"/>			</xs:extension>		</xs:complexContent>	</xs:complexType>	<xs:complexType name="SourceAppVersionInfoType">		<xs:complexContent>			<xs:extension base="bt:VersionInfoType">				<xs:attribute name="nameSourceApp" type="xs:string" use="required"/>				<xs:attribute name="uuidSourceApp" type="bt:uuidFormal" use="required"/>				<xs:attribute name="uuidSourceAppNode" type="bt:uuidFormal" use="required"/>				<xs:attribute name="uuidSession" type="bt:uuidFormal" use="required"/>			</xs:extension>		</xs:complexContent>	</xs:complexType>	<xs:complexType name="EntityReplicationType">		<xs:complexContent>			<xs:extension base="bt:EntityVersionType">				<xs:attribute name="erid" type="bt:erid" use="required"/>			</xs:extension>		</xs:complexContent>	</xs:complexType>	<xs:complexType name="EntityReplicationNamedType">		<xs:complexContent>			<xs:extension base="bt:EntityReplicationType">				<xs:attribute name="name" type="bt:nameType" use="required"/>			</xs:extension>		</xs:complexContent>	</xs:complexType>	<xs:complexType name="EntityReplicationNamedStateType">		<xs:complexContent>			<xs:extension base="bt:EntityReplicationNamedType">				<xs:attribute name="active" type="xs:boolean" use="optional" default="true"/>			</xs:extension>		</xs:complexContent>	</xs:complexType>	<xs:complexType name="EntityReplicationRefType">		<xs:attribute name="eridRef" type="bt:erid" use="required"/>	</xs:complexType>	<xs:complexType name="EntityType">		<xs:attribute name="id" type="bt:id" use="required"/>	</xs:complexType>	<xs:complexType name="EntityNamedType">		<xs:complexContent>			<xs:extension base="bt:EntityType">				<xs:attribute name="name" type="bt:nameType" use="required"/>			</xs:extension>		</xs:complexContent>	</xs:complexType>	<xs:complexType name="EntityRefType">		<xs:attribute name="idRef" type="bt:idRef" use="required"/>	</xs:complexType>	<xs:complexType name="EntityLocalType">		<xs:attribute name="id" type="xs:ID" use="required"/>	</xs:complexType>	<xs:complexType name="EntityLocalNamedType">		<xs:attribute name="id" type="xs:ID" use="required"/>		<xs:attribute name="name" type="bt:nameType" use="required"/>	</xs:complexType>	<xs:complexType name="EntityLocalRefType">		<xs:attribute name="idRef" type="xs:IDREF" use="required"/>	</xs:complexType>	<xs:complexType name="EntityDefinedType">		<xs:annotation>			<xs:documentation>This type of entity replication type will be used for items that come from JD AMS or reference servers with fixed ID's.  UUID's are used on most Entities since they will be created on potentially any node by any user.</xs:documentation>		</xs:annotation>		<xs:complexContent>			<xs:extension base="bt:EntityVersionType">				<xs:attribute name="id" type="bt:entityDefinedID" use="required"/>			</xs:extension>		</xs:complexContent>	</xs:complexType>	<xs:complexType name="EntityDefinedNamedType">		<xs:complexContent>			<xs:extension base="bt:EntityDefinedType">				<xs:attribute name="name" type="bt:nameType" use="required"/>			</xs:extension>		</xs:complexContent>	</xs:complexType>	<xs:complexType name="EntityDefinedNamedStateType">		<xs:complexContent>			<xs:extension base="bt:EntityDefinedNamedType">				<xs:attribute name="active" type="xs:boolean" use="optional" default="true"/>			</xs:extension>		</xs:complexContent>	</xs:complexType>	<xs:complexType name="EntityDefinedRefType">		<xs:attribute name="idRef" type="bt:entityDefinedID" use="required"/>	</xs:complexType>	<xs:complexType name="FileSchemaContentVersionInfoType">		<xs:sequence>			<xs:element name="FileSchemaContentVersion" type="bt:BasicVersionInfoType">				<xs:annotation>					<xs:documentation>The version number here reflects the content of the file based on the defined schema version understood at the time of its creation.Changes to this version number reflect not only file structure changes but also new or changed acceptable implied content.</xs:documentation>				</xs:annotation>			</xs:element>			<xs:element name="UnitOfMeasureVersion" type="bt:BasicVersionInfoType">				<xs:annotation>					<xs:documentation>This version number reflects the minimum unit of measures version referenced within the or data structure this version stamp applies to.The reading application should check this version number.  If the major version number is not expected, then there may be items in the file that cannot be interpreted.  Each reader should know what versions of the unit system it supports including migration necessary from older versions.</xs:documentation>				</xs:annotation>			</xs:element>			<xs:element name="RepresentationSystemVersion" type="bt:BasicVersionInfoType">				<xs:annotation>					<xs:documentation>This version number reflects the minimum RepresentationSystem version referenced within the or data structure this version stamp applies to.The reading application should check this version number.  If the major version number is not expected, then there may be items in the file that cannot be interpreted.  Each reader should know what versions of the representation system it supports including migration necessary from older versions.</xs:documentation>				</xs:annotation>			</xs:element>		</xs:sequence>		<xs:attribute name="nonProductionCode" type="xs:int" use="required">			<xs:annotation>				<xs:documentation>Set to 0 to signify that file version is production or intended for production, set to non-zero for each unique prototype or field test version within the same major, minor version number.</xs:documentation>			</xs:annotation>		</xs:attribute>	</xs:complexType>	<xs:simpleType name="localeCode">		<xs:annotation>			<xs:documentation>ISO 639-1 Language Code with ISO 3166 country code.</xs:documentation>		</xs:annotation>		<xs:restriction base="xs:string">			<xs:pattern value="[a-z]{2}-[A-Z]{2}"/>		</xs:restriction>	</xs:simpleType>	<xs:simpleType name="languageCode">		<xs:annotation>			<xs:documentation>ISO 639-1 Language Code</xs:documentation>		</xs:annotation>		<xs:restriction base="xs:string">			<xs:pattern value="[a-z]{2}"/>		</xs:restriction>	</xs:simpleType>	<xs:element name="NodeVersions">		<xs:complexType>			<xs:sequence>				<xs:element name="Node" maxOccurs="unbounded">					<xs:complexType>						<xs:sequence>							<xs:element name="ChangeException" minOccurs="0">								<xs:complexType>									<xs:attribute name="erid" type="bt:erid" use="required"/>								</xs:complexType>							</xs:element>						</xs:sequence>						<xs:attribute name="uuid" type="bt:uuid" use="required"/>						<xs:attribute name="lastSeen" type="xs:dateTime" use="required"/>					</xs:complexType>				</xs:element>			</xs:sequence>		</xs:complexType>	</xs:element>	<xs:element name="Synchronization">		<xs:complexType>			<xs:sequence>				<xs:element ref="bt:NodeVersions"/>				<xs:element ref="bt:EntityDeletions"/>			</xs:sequence>		</xs:complexType>	</xs:element>	<xs:element name="EntityDeletions">		<xs:complexType>			<xs:sequence>				<xs:element name="Entity" type="bt:erid" minOccurs="0" maxOccurs="unbounded"/>			</xs:sequence>		</xs:complexType>	</xs:element>	<xs:complexType name="EntityVersionType">		<xs:annotation>			<xs:documentation>This is the base complex type for all versioned and replicable or syncronizable entities that will exist the RCD &lt;-&gt; EIC Log files.</xs:documentation>		</xs:annotation>		<xs:attribute name="lastModified" type="xs:dateTime" use="required"/>		<xs:attribute name="sourceNode" type="bt:uuidFormal" use="required"/>	</xs:complexType>	<xs:simpleType name="longitude">		<xs:restriction base="xs:double">			<xs:maxInclusive value="180.0"/>			<xs:minInclusive value="-180.0"/>		</xs:restriction>	</xs:simpleType>	<xs:simpleType name="latitude">		<xs:restriction base="xs:double">			<xs:maxInclusive value="90.0"/>			<xs:minInclusive value="-90.0"/>		</xs:restriction>	</xs:simpleType>	<xs:simpleType name="varietyColor">		<xs:annotation>			<xs:documentation>Enumeration of colors (VT color index) to represent the varieties. 			The list of colors were selected by the Daytona embedded team.</xs:documentation>		</xs:annotation>		<xs:restriction base="xs:unsignedByte">			<xs:enumeration value="190">				<xs:annotation>					<xs:documentation>Variety #1, this would be displayed as the first variety on the GS2 screen. RGB values (204, 255, 0)</xs:documentation>				</xs:annotation>			</xs:enumeration>			<xs:enumeration value="130">				<xs:annotation>					<xs:documentation>Variety #2, this would be displayed as the second variety on the GS2 screen. RGB values (153, 51, 0)</xs:documentation>				</xs:annotation>			</xs:enumeration>			<xs:enumeration value="11">				<xs:annotation>					<xs:documentation>Variety #3, this would be displayed as the third variety on the GS2 screen. RGB values (0, 255, 255)</xs:documentation>				</xs:annotation>			</xs:enumeration>			<xs:enumeration value="216">				<xs:annotation>					<xs:documentation>Variety #4, this would be displayed as the fourth variety on the GS2 screen. RGB values (255, 153, 102)</xs:documentation>				</xs:annotation>			</xs:enumeration>			<xs:enumeration value="135">				<xs:annotation>					<xs:documentation>Variety #5, this would be displayed as the fifth variety on the GS2 screen. RGB values (153, 51, 255)</xs:documentation>				</xs:annotation>			</xs:enumeration>			<xs:enumeration value="137">				<xs:annotation>					<xs:documentation>Variety #6, this would be displayed as the sixth variety on the GS2 screen. RGB values (153, 102, 51)</xs:documentation>				</xs:annotation>			</xs:enumeration>			<xs:enumeration value="255">				<xs:annotation>					<xs:documentation>This represents 'Not Planted rows'. RGB values (255, 255, 182)</xs:documentation>				</xs:annotation>			</xs:enumeration>		</xs:restriction>	</xs:simpleType></xs:schema>